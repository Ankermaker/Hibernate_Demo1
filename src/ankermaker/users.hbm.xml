<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="ankermaker">

    <class name="Users" table="users"> 
        <id name="user_id" column="user_id" type="integer">
            <generator class="native"></generator>
        </id>
        <property name="user_name"  column="user_name" type="string"></property>
        <property name="user_six" column="user_six" type="string"></property>
        <property name="user_salry" column="user_salry" type="string"></property>
    </class>
    
</hibernate-mapping>